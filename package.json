{
  "name": "voicevox-blog",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "preview-build": "cross-env PREVIEW=1 astro build",
    "test-build": "cross-env TEST=1 astro build",
    "lint": "astro check && eslint --cache && prettier . --check --cache",
    "fmt": "eslint --fix --cache && prettier . --write --cache",
    "test:e2e": "cross-env TEST=1 playwright test",
    "generateThumb": "tsx ./src/tools/generateThumb.mts",
    "generateLatestDefaultEngineInfos": "tsx ./src/tools/generateLatestDefaultEngineInfos.ts",
    "updateVersion": "tsx ./src/tools/updateVersion.mts",
    "updateMarkdown": "tsx ./src/tools/updateMarkdown.mts",
    "updateSnapshots": "tsx ./src/tools/updateSnapshots.ts",
    "preinstall": "npx -y only-allow pnpm"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/react": "^4.4.2",
    "@astrojs/rss": "^4.0.14",
    "@astrojs/sitemap": "^3.6.1",
    "@eslint/js": "^9.39.2",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@nanostores/react": "^1.0.0",
    "@playwright/test": "^1.57.0",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/gtag.js": "^0.0.20",
    "@types/node": "^25.0.8",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/parser": "^8.53.0",
    "astro": "^5.16.9",
    "astro-seo": "^1.1.0",
    "bulma": "^1.0.4",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "nanostores": "^1.1.0",
    "playwright": "^1.57.0",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-astro-organize-imports": "^0.4.11",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sass": "^1.97.2",
    "semver": "^7.7.3",
    "sharp": "^0.34.5",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.3.1",
    "xml-formatter": "^3.6.7",
    "zod": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VOICEVOX/voicevox_blog"
  },
  "packageManager": "pnpm@10.18.3",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "sharp"
    ]
  }
}
